   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "df_ref = df[~df.duplicated(subset=[\"store_id\"])]  # 중복 제거\n",
    "df_ref = df_ref[df_ref[\"comments\"] >= 100]\n",
    "market_counts = df_ref.groupby(\"market\").count()[\"store_id\"]\n",
    "\n",
    "# count 값이 5 이상인 market 필터링\n",
    "markets = market_counts[market_counts >= 5].sort_values(ascending=False)\n",
    "# print(markets, '\\ntotal:', markets.sum())\n",
    "df_ftd = df_ref[df_ref[\"market\"].isin(markets.index)]\n",
    "\n"
   ]
